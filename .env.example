# Portfolio Environment Variables
# Copy this to .env and fill in your values

# 3D Model URL - For local development, use a local path:
# VITE_MODEL_URL=/models/your_model.glb

# For production (GitHub Pages), use a CDN URL:
# Option 1: jsDelivr CDN (from GitHub Releases)
# VITE_MODEL_URL=https://cdn.jsdelivr.net/gh/YOUR_USERNAME/YOUR_REPO@v1.0.0/cyber_samurai.glb

# Option 2: Direct URL from file hosting service
# VITE_MODEL_URL=https://your-cdn.com/path/to/model.glb

# Sketchfab UID (if using Sketchfab embed instead)
VITE_SKETCHFAB_UID=

# Model license/attribution HTML
VITE_MODEL_LICENSE_HTML=<p>3D Model attribution here</p>
